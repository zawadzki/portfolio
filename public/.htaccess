#AddHandler application/x-httpd-php82 .php
RewriteEngine On
RewriteCond %{HTTPS} !=on
RewriteRule ^ https://%{HTTP_HOST}%{REQUEST_URI} [L,R=301]

<FilesMatch "^\.env">
  Require all denied
</FilesMatch>

<FilesMatch "\.log$">
  Require all denied
</FilesMatch>

RedirectMatch 301 ^/portfolio/?$ /#work